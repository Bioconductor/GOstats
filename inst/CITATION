citHeader("To cite GOstats in publications use:")

citEntry(entry="INPROCEEDINGS",
  AUTHOR =       "Robert Gentleman",
  TITLE =        "Using {GO} for statistical analyses",
  BOOKTITLE =    "Compstat 2004 -- Proceedings in Computational Statistics",
  PAGES =        "171--180",
  YEAR =         "2004",
  EDITOR =       "Jaromir Antoch", 
        address = "Heidelberg",
  PUBLISHER =    "Physica Verlag, Heidelberg, Germany",

  textVersion = 
    paste("Using {GO} for statistical analyses, R. Gentleman,",
     "Compstat 2004 Proceedings in Computational Statistics;", 
     "171-180, 2004, Jaromir Antoch, (Ed), Physica Verlag")
)
